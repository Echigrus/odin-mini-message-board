<!-- index.ejs -->
<html>
  <head>
    <title>Mini Messageboard</title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <%- include('navbar', {links: links}) %>
    <div class="messageBoard">
      <div class="messageList">
        <% messages.forEach((msg) => { %> <%- include('message', { user:
        msg.user, added: msg.added, text: msg.text }); %><% }) %>
      </div>
    </div>
    <%- include('footer') %>
  </body>
</html>
